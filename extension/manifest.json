{
    "name": "React Chrome Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [
          "content.js"
        ]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/images/*"
        ],
        "matches": ["<all_urls>"]
      }
    ]
  }