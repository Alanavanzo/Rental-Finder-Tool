{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "jsx": "react",
      "baseUrl": ".",
      "paths": {
        "@components/*": ["src/components/*"],
        "@utils/*": ["src/utils/*"]
      },
      "allowSyntheticDefaultImports": true,
      "checkJs": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }